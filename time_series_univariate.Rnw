# plot in bottom panel
chart_Series(x=env_etf$XLF["2009-02/2009-04"], 
             name="XLF")
      @
    \column{0.5\textwidth}
    \vspace{-1em}
      \includegraphics[width=0.5\paperwidth,valign=t]{figure/chart_Series_panels-1}
  \end{columns}
\end{block}

